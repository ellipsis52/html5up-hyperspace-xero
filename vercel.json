{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "html5up-hyperspace" }
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/callback", "dest": "api/server.js" },
    { "src": "/api/cron", "dest": "api/cron.js" },
    { "src": "/", "dest": "/html5up-hyperspace/index.html" },
    { "src": "/(.*)", "dest": "/html5up-hyperspace/$1" }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 10 * * *"
    }
  ]
}
{
  "routes": [
    { "src": "/signup", "dest": "/html5up-hyperspace/signup.html" },
    { "src": "/(.*)", "dest": "/html5up-hyperspace/$1" }
  ]
}
"builds": [
  { "src": "html5up-hyperspace/**", "use": "@vercel/static" }
]
